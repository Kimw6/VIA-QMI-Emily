<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" fx:id="addBeadsPage" prefHeight="400.0" prefWidth="677.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.AddBeadsPageController">
    <stylesheets>
        <URL value="@inputData.css" />
    </stylesheets>
   <children>
      <Text layoutX="49.0" layoutY="40.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Add Beads from Beads List ">
         <font>
            <Font size="22.0" />
         </font>
      </Text>
      <Text layoutX="42.0" layoutY="97.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Probes" />
      <Text layoutX="354.0" layoutY="97.0" strokeType="OUTSIDE" strokeWidth="0.0" text="choosen probes for Experiment" />
      <Button fx:id="addBeadsBtn" layoutX="264.0" layoutY="200.0" mnemonicParsing="false" onAction="#addProbeAction" text="ADD &gt;&gt;">
         <font>
            <Font name="System Bold Italic" size="14.0" />
         </font>
      </Button>
      <TableView fx:id="probeTable" layoutX="42.0" layoutY="115.0" prefHeight="200.0" prefWidth="209.0">
        <columns>
          <TableColumn fx:id="probeNoCol" prefWidth="75.0" text="No." />
          <TableColumn fx:id="probeNameCol" prefWidth="132.0" text="Probes " />
        </columns>
      </TableView>
      <Button fx:id="Delete" layoutX="557.0" layoutY="200.0" mnemonicParsing="false" onAction="#deleteEvent" prefHeight="25.0" prefWidth="81.0" text="Delete" />
      <Button fx:id="moveUpBtn" layoutX="557.0" layoutY="235.0" mnemonicParsing="false" onAction="#moveUpEvent" prefHeight="25.0" prefWidth="81.0" text="Move Up" />
      <Button fx:id="moveDownBtn" layoutX="557.0" layoutY="269.0" mnemonicParsing="false" onAction="#moveDownEvent" text="Move Down" />
      <TableView fx:id="experimentProbeTable" layoutX="346.0" layoutY="115.0" prefHeight="200.0" prefWidth="200.0">
        <columns>
          <TableColumn fx:id="experimentProbeNoCol" prefWidth="94.0" text="No." />
          <TableColumn fx:id="experimentProbeNameCol" prefWidth="105.0" text="Probe Name" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
