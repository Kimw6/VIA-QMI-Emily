<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" fx:id="addBeadsPage" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="javafxapplication1.AddBeadsController">
    <stylesheets>
        <URL value="@inputData.css" />
    </stylesheets>
   <children>
      <Text layoutX="49.0" layoutY="40.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Add Beads from Beads List ">
         <font>
            <Font size="22.0" />
         </font>
      </Text>
      <Text layoutX="49.0" layoutY="97.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Beads Class List" />
      <Text layoutX="354.0" layoutY="97.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Experiment beads" />
      <Button fx:id="addBeadsBtn" layoutX="223.0" layoutY="200.0" mnemonicParsing="false" onAction="#addBeadAction" text="ADD &gt;&gt;">
         <font>
            <Font name="System Bold Italic" size="14.0" />
         </font>
      </Button>
      <Button fx:id="deleteAnalyteFromExp" layoutX="444.0" layoutY="323.0" mnemonicParsing="false" onAction="#deleteBeadAction" text="Delete" />
      <TableView fx:id="beadsListTable" layoutX="46.0" layoutY="120.0" prefHeight="200.0" prefWidth="152.0">
        <columns>
          <TableColumn fx:id="beadClassCol" prefWidth="75.0" text="Bead Class" />
          <TableColumn fx:id="analyteCol" prefWidth="75.0" text="Analyte" />
        </columns>
      </TableView>
      <TableView fx:id="experimentBeadsTable" layoutX="348.0" layoutY="115.0" prefHeight="200.0" prefWidth="164.0">
        <columns>
          <TableColumn fx:id="experimentBeadClassCol" prefWidth="85.0" text="Bead Class" />
            <TableColumn fx:id="experimentAnalyteCol" prefWidth="65.0" text="Analyte" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
